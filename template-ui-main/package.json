{
  "name": "@template-ui/main",
  "scripts": {
    "clean": "rimraf -rf dist/ coverage/ .serverless/",
    "prepare": "is-ci || husky install",
    "pre-commit": "lint-staged && npm test --watchAll=false",
    "start": "better-npm-run start:local",
    "start:np": "better-npm-run start:np",
    "start:webpack": "webpack serve --port 9000 --mode=development --node-env=development",
    "lint": "eslint src --fix --ext js",
    "pretest": "npm run clean && npm run lint",
    "test": "jest",
    "test:ci": "better-npm-run test:ci",
    "test:sls": "sls package --verbose -r us-west-2 -s np",
    "prebuild": "npm run clean",
    "build": "webpack --mode=production --node-env=production --progress --color",
    "build:np:w": "better-npm-run build:np:w",
    "build:np:e": "better-npm-run build:np:e",
    "build:prd:w": "better-npm-run build:prd:w",
    "build:prd:e": "better-npm-run build:prd:e",
    "dp:np:w": "sls deploy --verbose -r us-west-2 -s np --force",
    "dp:np:e": "sls deploy --verbose -r us-east-1 -s np --force",
    "dp:prd:w": "sls deploy --verbose -r us-west-2 -s prd --force",
    "dp:prd:e": "sls deploy --verbose -r us-east-1 -s prd --force",
    "dp:np:w:ci": "eval \"$(assume-role) npm run dp:np:w\"",
    "dp:np:e:ci": "eval \"$(assume-role) npm run dp:np:e\"",
    "dp:prd:w:ci": "eval \"$(assume-role) npm run dp:prd:w\"",
    "dp:prd:e:ci": "eval \"$(assume-role) npm run dp:prd:e\""
  },
  "betterScripts": {
    "start:local": {
      "command": "npm run start:webpack",
      "env": {
        "ENV": "np",
        "AWS_REGION": "us-west-2",
        "COMMENTED_OUT-IMPORTMAP_METADATA": "http://localhost:9000/fixtures/importmap-local-fixtures.json",
        "COMMENTED_OUT-MICRO_APP_METADATA": "http://localhost:9000/fixtures/local-app-fixtures.json"
      }
    },
    "start:np": {
      "command": "npm run start:webpack",
      "env": {
        "ENV": "np",
        "AWS_REGION": "us-west-2"
      }
    },
    "build:np:e": {
      "command": "npm run build",
      "env": {
        "ENV": "np",
        "AWS_REGION": "us-east-1"
      }
    },
    "build:np:w": {
      "command": "npm run build",
      "env": {
        "ENV": "np",
        "AWS_REGION": "us-west-2"
      }
    },
    "build:prd:e": {
      "command": "npm run build",
      "env": {
        "ENV": "prd",
        "AWS_REGION": "us-east-1"
      }
    },
    "build:prd:w": {
      "command": "npm run build",
      "env": {
        "ENV": "prd",
        "AWS_REGION": "us-west-2"
      }
    },
    "test:ci": {
      "command": "jest --coverage --testResultsProcessor",
      "env": {
        "CI": "true"
      }
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.12.0",
    "@babel/eslint-plugin": "^7.12.0",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.0",
    "@babel/plugin-syntax-import-meta": "^7.10.0",
    "@babel/plugin-transform-react-constant-elements": "^7.12.1",
    "@babel/plugin-transform-react-inline-elements": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/register": "^7.12.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "aws-assume-role-cicd": "^1.0.4",
    "babel-jest": "^28.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "better-npm-run": "^0.1.1",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^7.0.0",
    "enzyme": "^3.11.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^27.2.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "is-ci": "^3.0.0",
    "jest": "^28.1.0",
    "jest-cli": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-localstorage-mock": "^2.4.2",
    "jest-react-hooks-shallow": "^1.5.1",
    "lint-staged": "^12.4.1",
    "nock": "^13.1.0",
    "prettier": "^2.0.5",
    "serve": "^11.3.2",
    "serverless": "^3.34.0",
    "serverless-spa-config": "^1.0.12",
    "serverless-spa-deploy": "^1.0.2",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.9.0",
    "webpack-config-single-spa-react": "^4.0.3",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0",
    "whatwg-fetch": "^3.6.2",
    "workbox-webpack-plugin": "^6.5.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,md}": [
      "prettier --write",
      "eslint --fix-dry-run"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.15",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.18",
    "@testing-library/react-hooks": "^8.0.1",
    "import-map-overrides": "2.2.0",
    "moment": "2.29.3",
    "prop-types": "^15.7.2",
    "pubsub-js": "^1.9.3",
    "react": "17.0.2",
    "react-bootstrap": "2.3.1",
    "react-bootstrap-icons": "^1.0.0",
    "react-bootstrap-typeahead": "5.2.0",
    "react-datepicker": "3.7.0",
    "react-dom": "17.0.2",
    "react-hook-form": "7.31.0",
    "react-query": "3.39.1",
    "react-router-dom": "6.3.0",
    "react-select": "^3.1.0",
    "react-table": "7.7.0",
    "react-toastify": "9.0.4",
    "regenerator-runtime": "0.13.9",
    "single-spa": "5.9.3",
    "single-spa-layout": "^1.4.1",
    "systemjs": "6.12.1",
    "workbox-background-sync": "^6.5.3",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^6.5.3",
    "workbox-expiration": "^7.0.0",
    "workbox-precaching": "^6.5.3",
    "workbox-recipes": "^6.5.3",
    "workbox-routing": "^6.5.3",
    "workbox-strategies": "^6.5.3"
  }
}
